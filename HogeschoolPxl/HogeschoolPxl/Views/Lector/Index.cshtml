@model IEnumerable<HogeschoolPxl.Models.Lector>

@{
    ViewData["Title"] = "Lectoren";
}
@section LectorenStyle{
    <link rel="stylesheet" href="~/css/Lectoren.css" />
}
<h3 class="cat-heading">Lectoren</h3>

<p>
    <a asp-action="Create">Create New</a>
</p>
<div class="lectoren">
    <div>
        @foreach (Lector lector in Model)
        {
            <partial name="LectorCard" model="@lector" />
        }
    </div>
</div>
