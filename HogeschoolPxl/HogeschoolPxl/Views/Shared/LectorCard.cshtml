@model Lector
@{ 
    var photoPath = "/images/" + (Model.Gebruiker.ImageUrl ?? "nogebruiker.png");
}
<section>
    <img src="@photoPath" asp-append-version="true" alt="palmaers">
    <span>@Model.Gebruiker.Naam</span>
    <div class="links">
        <a asp-action="Edit" asp-route-id="@Model.LectorId" class="btn btn-primary">Edit</a>
        <a asp-action="Details" asp-route-id="@Model.LectorId" class="btn btn-info">Details</a>
        <a asp-action="Delete" asp-route-id="@Model.LectorId" class="btn btn-danger">Delete</a>
    </div>
</section>