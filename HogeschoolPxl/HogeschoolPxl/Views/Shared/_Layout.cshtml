@inject SignInManager<Gebruiker> signInManager;
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HogeschoolPxl</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/Main.css" />

    @if (IsSectionDefined("GebruikerStyle"))
    {
        @RenderSection("GebruikerStyle", required: true);
    }
    @if (IsSectionDefined("LectorenStyle"))
    {
        @RenderSection("LectorenStyle", required: true);
    }
    @if (IsSectionDefined("InschrijvingStyle"))
    {
        @RenderSection("InschrijvingStyle", required: true);
    }
</head>
<body>
    <header class="bg-light">
        <div class="container">
            <nav class="navbar navbar-expand-lg navbar-light">
                <a class="navbar-brand" asp-controller="Home" asp-action="Index">HogeschoolPxl</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav">
                        <partial name="Links"/>
                    </ul>
                </div>
            </nav>
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - HogeschoolPxl - <a asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/Main.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
    @if (IsSectionDefined("ShowPath"))
    {
        @RenderSection("ShowPath", required: true);
    }
    @if (IsSectionDefined("scriptsAjax"))
    {
        @RenderSection("scriptsAjax", required: true);
    }


</body>
</html>
